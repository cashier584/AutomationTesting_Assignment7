{
    "configuration": {
        "device": {
            "orientation": "landscape",
            "deviceName": "Desktop"
        }
    },
    "type": "clickpath",
    "version": "1.0",
    "events": [
        {
            "description": "Loading of \"https://dailydictation.com\"",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {},
            "type": "navigate",
            "url": "https://dailydictation.com"
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "a:contains(\"Login\")"
                    }
                ]
            },
            "type": "click",
            "button": 0
        },
        {
            "description": "Loading of \"https://dailydictation.com/login\"",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {},
            "type": "navigate",
            "url": "https://dailydictation.com/login"
        },
        {
            "description": "type hiepvvip45@gmail.com",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#_username"
                    }
                ]
            },
            "type": "keystrokes",
            "textValue": "hiepvvip45@gmail.com",
            "simulateBlurEvent": true,
            "masked": false
        },
        {
            "description": "type HAOvo123@",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#_password"
                    }
                ]
            },
            "type": "keystrokes",
            "textValue": "HAOvo123@",
            "simulateBlurEvent": true,
            "masked": false
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "css",
                        "value": "#submit"
                    }
                ]
            },
            "type": "click",
            "button": 0
        },
        {
            "description": "Loading of \"https://dailydictation.com/exercises\"",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {},
            "type": "navigate",
            "url": "https://dailydictation.com/exercises"
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "dom",
                        "value": "document.evaluate('//li[@id=\\'app-user-notes\\']/a/span', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
                    }
                ]
            },
            "type": "click",
            "button": 0
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "dom",
                        "value": "document.evaluate('//div[2]/div/div/button/i', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
                    }
                ]
            },
            "type": "click",
            "button": 0
        },
        {
            "description": "type test",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "dom",
                        "value": "document.evaluate('(.//*[normalize-space(text()) and normalize-space(.)=\\'Add a new note\\'])[1]/following::textarea[1]', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
                    }
                ]
            },
            "type": "keystrokes",
            "textValue": "test",
            "simulateBlurEvent": true,
            "masked": false
        },
        {
            "description": "click",
            "wait": {
                "waitFor": "page_complete"
            },
            "target": {
                "locators": [
                    {
                        "type": "dom",
                        "value": "document.evaluate('//button[@type=\\'submit\\']', document, null, XPathResult.ANY_TYPE, null).iterateNext()"
                    }
                ]
            },
            "type": "click",
            "button": 0
        }
    ]
}